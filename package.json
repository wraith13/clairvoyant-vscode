{
	"name": "clairvoyant",
	"displayName": "clairvoyant",
	"description": "Smart search your codes",
	"version": "0.0.1",
	"publisher": "wraith13",
	"license": "SEE LICENSE IN LICENSE_1_0.txt",
	"engines": {
		"vscode": "^1.39.0"
	},
	"bugs": {
		"url": "https://github.com/wraith13/clairvoyant-vscode/issues"
	},
	"homepage": "https://github.com/wraith13/clairvoyant-vscode",
	"repository": {
		"type": "git",
		"url": "https://github.com/wraith13/clairvoyant-vscode.git"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "clairvoyant.scanDocument",
				"title": "%clairvoyant.scanDocument.title%",
				"category": "clairvoyant"
			},
			{
				"command": "clairvoyant.scanOpenDocuments",
				"title": "%clairvoyant.scanOpenDocuments.title%",
				"category": "clairvoyant"
			},
			{
				"command": "clairvoyant.scanFolder",
				"title": "%clairvoyant.scanFolder.title%",
				"category": "clairvoyant"
			},
			{
				"command": "clairvoyant.reload",
				"title": "%clairvoyant.reload.title%",
				"category": "clairvoyant"
			}
		],
		"configuration":[
			{
				"title": "clairvoyant",
				"properties": {
					"backgroundPhiColors.autoScanMode": {
						"type": "string",
						"default": "folder",
						"enum": [
							"none",
							"current document",
							"open documents",
							"folder"
						],
						"overridable": true,
						"description": "%clairvoyant.autoScanMode.description%"
					}
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	}
}
